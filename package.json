		"clsx": "^2.1.1",
		"dotenv": "^17.2.3",
		"ioredis": "^5.8.2",
		"lucide-react": "^0.548.0",
		"lucide-svelte": "^0.553.0",
		"prisma": "6.19.0",
		"qrcode-generator": "^2.0.4",
		"tailwind-merge": "^3.3.1",
		"uuid": "^13.0.0",
		"zod": "^4.1.12",
		"zod-form-data": "^3.0.1"
	},
	"trustedDependencies": [
		"@prisma/client"
	]
}
