{
	"name": "mindforge",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:queueonly": "docker compose -f docker-compose.dev.yaml up --build worker redis",
		"dev:dbonly": "docker compose -f docker-compose.dev.yaml up --build db",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:all": "vitest",
		"test:unit": "vitest --project unit",
		"test:integration": "docker compose -f docker-compose.dev.yaml build worker && vitest --project integration",
		"test:long": "vitest --project long-running",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.1",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.544.0",
		"@playwright/test": "^1.49.1",
		"@storybook/addon-a11y": "^9.1.15",
		"@storybook/addon-docs": "^9.1.15",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/addon-vitest": "^9.1.15",
		"@storybook/sveltekit": "^9.1.15",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@testcontainers/redis": "^11.8.0",
		"@types/bun": "^1.3.2",
		"@types/node": "^24",
		"@vitest/browser": "^4.0.8",
		"@vitest/ui": "4.0.8",
		"bits-ui": "^2.11.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-storybook": "^9.1.15",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"storybook": "^9.1.15",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.0.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.2.2",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.8",
		"vitest-browser-svelte": "^2.0.1"
	},
	"dependencies": {
		"@openrouter/ai-sdk-provider": "^1.2.0",
		"@prisma/client": "6.19.0",
		"@testcontainers/postgresql": "^11.7.2",
		"@trpc/client": "11.7.1",
		"@trpc/server": "11.7.1",
		"ai": "^5.0.81",
		"better-auth": "^1.3.33",
